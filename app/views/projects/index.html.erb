 <p id="notice"><%= notice %></p>

    <h1>Projects</h1>

    <table>
      <thead>
        <tr>
          <th>Work</th>
          <th>Artist</th>
          <th>Year</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @projects.each do |project| %>
          <tr>
            <td><img src= "<%= project.work %>" style= "width: 400px"></td>
            <td><%= project.artist %></td>
            <td><%= project.year %></td>
            <td><%= link_to 'Show', project %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Project', new_project_path %>
    <%= link_to 'Voltar', root_path %>
